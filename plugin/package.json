{
	"name": "vite-plugin-dotenv-in-html",
	"version": "1.0.0",
	"description": "A simple ViteJS plugin to replace .env variables in index.html",
	"author": "Yvan Razafindramanana",
	"license": "Apache-2.0",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "unbuild",
		"prepare": "npm run build",
		"test": "vitest watch",
		"lint": "eslint src/**/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yvzn/vite-plugin-dotenv-in-html.git"
	},
	"bugs": {
		"url": "https://github.com/yvzn/vite-plugin-dotenv-in-html/issues"
	},
	"homepage": "https://github.com/yvzn/vite-plugin-dotenv-in-html#readme",
	"devDependencies": {
		"eslint": "^8.23.1",
		"unbuild": "^0.8.11",
		"vite": "^3.1.0",
		"vitest": "^0.23.2"
	},
	"peerDependencies": {
		"vite": "3"
	}
}
