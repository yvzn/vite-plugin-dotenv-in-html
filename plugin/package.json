{
	"name": "vite-plugin-dotenv-in-html",
	"version": "4.0.0",
	"description": "A simple ViteJS plugin to replace .env variables in index.html",
	"author": "Yvan Razafindramanana",
	"license": "Apache-2.0",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "unbuild",
		"prepare": "npm run build",
		"test": "vitest run",
		"lint": "eslint src/**/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yvzn/vite-plugin-dotenv-in-html.git"
	},
	"bugs": {
		"url": "https://github.com/yvzn/vite-plugin-dotenv-in-html/issues"
	},
	"homepage": "https://github.com/yvzn/vite-plugin-dotenv-in-html#readme",
	"devDependencies": {
		"eslint": "^8.31.0",
		"unbuild": "^1.0.2",
		"vite": "^4.0.3",
		"vitest": "^0.26.3"
	},
	"peerDependencies": {
		"vite": "4"
	}
}
